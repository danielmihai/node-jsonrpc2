{
  "name": "json-rpc2",
  "version": "2.0.0",
  "description": "JSON-RPC 2.0 server and client library, with HTTP, TCP and Websocket endpoints",
  "main": "./src/jsonrpc.js",
  "keywords": [
    "json",
    "rpc",
    "rpc2",
    "json-rpc",
    "json-rpc2",
    "jsonrpc",
    "jsonrpc2",
    "server",
    "client",
    "tcp",
    "websocket",
    "http"
  ],
  "license": "MIT",
  "author": {
    "name": "Eric Florenzano",
    "url": "eflorenzano.com"
  },
  "maintainers": [
    {
      "name": "Paulo Cesar",
      "url": "https://github.com/pocesar"
    }
  ],
  "dependencies": {
    "bluebird": "^3.4.6",
    "debug": "^2.2.0",
    "es5class": "^2.3.1",
    "eventemitter3": "^2.0.2",
    "faye-websocket": "^0.11.0",
    "jsonparse": "^1.2.0",
    "lodash": "^4.16.4",
    "object-assign": "^4.1.0"
  },
  "devDependencies": {
    "mocha": "^3.1.0",
    "expect.js": "^0.3.1",
    "jshint": "^2.9.3",
    "istanbul": "^0.4.5",
    "coveralls": "^2.11.14",
    "mocha-lcov-reporter": "^1.2.0"
  },
  "engines": {
    "node": "4.x || 6.x"
  },
  "contributors": [
    "Bill Casarin <bill@casarin.ca> (jb55.com)",
    "Stefan Thomas <justmoon@members.fsf.org> (justmoon.net)",
    "Paulo Cesar <email@pocesar.e4ward.com> (github.com/pocesar)"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/pocesar/node-jsonrpc2.git"
  },
  "bugs": {
    "url": "https://github.com/pocesar/node-jsonrpc2/issues"
  },
  "scripts": {
    "test": "jshint examples src test && mocha test/*.js",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha -- -t 5000 test/jsonrpc-*.js",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec test/jsonrpc-*.js && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  }
